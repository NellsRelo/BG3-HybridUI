                                                <b:Interaction.Triggers>
                                                    <b:EventTrigger EventName="LSInputBindingReleased">
                                                        <b:ChangePropertyAction TargetName="gridScrollViewer" PropertyName="Visibility" Value="Visible"/>
                                                        <ls:SetMoveFocusAction TargetName="CharacterCreation_c" FocusElement="{Binding ElementName=itemGrid}"/>
                                                        <ls:LSPlaySound Sound="UI_DefaultButton_Click"/>
                                                    </b:EventTrigger>
                                                </b:Interaction.Triggers>
                                            </ls:LSInputBinding>
                                            
                                            <!-- Navigate down to grid when UIDown is pressed and grid is visible -->
                                            <ls:LSInputBinding BoundEvent="UIDown" IsEnabled="{Binding ElementName=gridScrollViewer, Path=IsVisible}">
                                                <b:Interaction.Triggers>
                                                    <b:EventTrigger EventName="LSInputBindingPressed">
                                                        <ls:SetMoveFocusAction TargetName="CharacterCreation_c" FocusElement="{Binding ElementName=itemGrid}"/>
                                                    </b:EventTrigger>
                                                </b:Interaction.Triggers>
                                            </ls:LSInputBinding>
