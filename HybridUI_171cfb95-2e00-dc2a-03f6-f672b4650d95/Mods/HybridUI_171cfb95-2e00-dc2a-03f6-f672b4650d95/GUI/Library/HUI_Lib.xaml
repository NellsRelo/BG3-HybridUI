    <!-- HUI Color Grid Bottom Buttons Template -->
    <!-- Extends AppearanceItemSelectedBottomButtons with color options toggle -->
    <ControlTemplate x:Key="HUI_ColorGridBottomButtons">
        <StackPanel Orientation="Horizontal">
            <!-- Standard appearance buttons -->
            <ls:LSButton x:Name="selectButton" ContentTemplate="{StaticResource ControllerButtonHint}" Content="{Binding CurrentPlayer.UIData.InputEvents, ConverterParameter=UIAccept, Converter={StaticResource FindInputEventConverter}}" Tag="{Binding Source=hd0c08193ga7f2g4a7eg8225g63748c7b3096,Converter={StaticResource TranslatedStringConverter}}" OverwriteClickSound="UI_DefaultButton_Click"/>

            <!-- Toggle tooltips -->
            <ls:LSButton x:Name="ToggleTooltips"
                         Command="ls:UIWidget.ToggleTooltipOnUIElementCommand"
                         CommandParameter="{Binding FocusedElement, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ls:UIWidget}}"
                         BoundEvent="UIShowInfo"
                         ContentTemplate="{StaticResource ControllerButtonHint}"
                         Content="{Binding CurrentPlayer.UIData.InputEvents, ConverterParameter=UIShowInfo, Converter={StaticResource FindInputEventConverter}}"
                         Tag="{Binding Source='hf8dd4c2fg97e0g4ccdgac1ag3cd1fe5060ef', Converter={StaticResource TranslatedStringConverter}}" TapTime="{StaticResource DefaultButtonTapTime}"/>

            <!-- All Colors Toggle Button - binds to the SkinColorOptions toggle -->
            <ls:LSButton x:Name="colorToggleButton"
                         BoundEvent="JumpToFromTab"
                         ContentTemplate="{StaticResource ControllerButtonHint}"
                         Content="{Binding CurrentPlayer.UIData.InputEvents, ConverterParameter=JumpToFromTab, Converter={StaticResource FindInputEventConverter}}"
                         Tag="{Binding Source='h4b7b0c9fg6a2fg4e1fg8b4fg123456789abc', Converter={StaticResource TranslatedStringConverter}, FallbackValue='All Colors'}"
                         DataContext="{Binding SkinColorOptions[0]}"
                         Command="{Binding DataContext.ValueChanged, RelativeSource={RelativeSource AncestorType={x:Type ls:UIWidget}}}"
                         CommandParameter="{Binding .}"
                         OverwriteClickSound="UI_DefaultButton_Click"/>

            <ls:LSButton x:Name="backButton" ContentTemplate="{StaticResource ControllerButtonHint}" Content="{Binding CurrentPlayer.UIData.InputEvents, ConverterParameter=UICancel, Converter={StaticResource FindInputEventConverter}}" Tag="{Binding Source=h77fec760g866dg4e47g8e81g467c9c05e447,Converter={StaticResource TranslatedStringConverter}}" OverwriteClickSound="UI_DefaultButton_Click"/>
        </StackPanel>

        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding ShowUIElementTooltip, RelativeSource={RelativeSource AncestorType={x:Type ls:UIWidget}}}" Value="True">
                <Setter TargetName="ToggleTooltips" Property="Tag" Value="{Binding Source='hb33ec471gd611g4ee3gbbffg699af8f79167', Converter={StaticResource TranslatedStringConverter}}"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!-- HUI_SliderTemplate - Hybrid Desktop/Controller Style slider -->
    <ControlTemplate x:Key="HUI_SliderTemplate" TargetType="Slider">
        <ControlTemplate.Resources>
            <BitmapImage x:Key="SliderBackground" UriSource="pack://application:,,,/Core;component/Assets/Options/sliderBox_wide_d.png"/>
        </ControlTemplate.Resources>
        <StackPanel ls:MoveFocus.Focusable="True" Focusable="True" 
                   Width="{StaticResource gameplayPanelWidth}" HorizontalAlignment="Center">
                <b:Interaction.Triggers>
                    <b:PropertyChangedTrigger Binding="{Binding Value, RelativeSource={RelativeSource TemplatedParent}}">
                        <ls:LSPlaySound Sound="UI_HUD_SplitItem_Step"/>
                        <b:InvokeCommandAction Command="{Binding DataContext.AppearanceValueChanged, RelativeSource={RelativeSource AncestorType={x:Type ls:UIWidget}}}"/>
                    </b:PropertyChangedTrigger>
                </b:Interaction.Triggers>
            
            <!-- Header with subheader and value -->
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,0,0,20">
                <ContentControl x:Name="PART_SubHeader" Template="{StaticResource HUI_Decorated_Subheader}">
                    <TextBlock x:Name="PART_SubHeaderText" 
                              Text="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" 
                              Style="{StaticResource AppearanceCarouselTitleText}"/>
                </ContentControl>
                
                <TextBlock x:Name="PART_ValueLabel" 
                          Text="{Binding Value, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource RoundDownConverter}}"
                          Foreground="{StaticResource LS_accent100TxtColor}" 
                          FontSize="{StaticResource ScaledMediumFontSize}"
                          Margin="40,0,0,0" VerticalAlignment="Center"
                          HorizontalAlignment="Right" TextAlignment="Left"/>
            </StackPanel>
            
                <!-- Main slider area uses full width -->
                <Grid x:Name="sliderContainer" HorizontalAlignment="Stretch" Height="105" 
                      Width="{StaticResource gameplayPanelWidth}">
                    
                    <!-- Focus effect background -->
                    <Rectangle x:Name="focusedFill" Fill="{StaticResource focusedFillGradiant}" 
                              Visibility="Collapsed" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                    
                    <ls:LSNineSliceImage x:Name="bg" HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                        ImageSource="{StaticResource SliderBackground}" 
                                        Height="105" Slices="106,20"/>

                <!-- Left and right buttons overlapping frame edges -->
                <ls:LSRepeatButton x:Name="PART_DecreaseRepeatButton" HorizontalAlignment="Left" 
                                  Style="{StaticResource SliderRepeatButtonLeftStyle}" 
                                  Width="105" Height="105" Margin="-15,0,0,0"
                                  BoundEvent="UILeft" Command="Slider.DecreaseSmall"
                                  IsEnabled="{Binding Path=(ls:MoveFocus.IsFocused), RelativeSource={RelativeSource TemplatedParent}}" />
                <ls:LSRepeatButton x:Name="PART_IncreaseRepeatButton" HorizontalAlignment="Right" 
                                  Style="{StaticResource SliderRepeatButtonRightStyle}"
                                  Width="105" Height="105" Margin="0,0,-15,0"
                                  BoundEvent="UIRight" Command="Slider.IncreaseSmall"
                                  IsEnabled="{Binding Path=(ls:MoveFocus.IsFocused), RelativeSource={RelativeSource TemplatedParent}}" />

                <!-- Track with proper spacing for thumb positioning -->
                <Track Name="PART_Track" HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                       Margin="105,0,105,0" Height="105">
                    <Track.DecreaseRepeatButton>
                        <RepeatButton Opacity="0" IsHitTestVisible="False" Command="Slider.DecreaseSmall"/>
                    </Track.DecreaseRepeatButton>
                    <Track.Thumb>
                        <ls:LSThumb x:Name="HighlightTrack" Style="{StaticResource OptionSliderThumbStyle}" 
                                   Width="108" Height="105" VerticalAlignment="Center"/>
                    </Track.Thumb>
                    <Track.IncreaseRepeatButton>
                        <RepeatButton Opacity="0" IsHitTestVisible="False" Command="Slider.IncreaseSmall"/>
                    </Track.IncreaseRepeatButton>
                </Track>
                </Grid>
            </StackPanel>
        <ControlTemplate.Triggers>
            <!-- Focus effect trigger -->
            <DataTrigger Binding="{Binding Path=(ls:MoveFocus.IsFocused), RelativeSource={RelativeSource Self}}" Value="True">
                <Setter TargetName="focusedFill" Property="Visibility" Value="Visible"/>
            </DataTrigger>
            
            <!-- Disabled appearance when not focused - dim buttons but keep thumb opaque -->
            <DataTrigger Binding="{Binding Path=(ls:MoveFocus.IsFocused), RelativeSource={RelativeSource Self}}" Value="False">
                <Setter TargetName="PART_DecreaseRepeatButton" Property="Opacity" Value="0.4"/>
                <Setter TargetName="PART_IncreaseRepeatButton" Property="Opacity" Value="0.4"/>
                <Setter TargetName="PART_Track" Property="IsEnabled" Value="False"/>
            </DataTrigger>
            
            <DataTrigger Binding="{Binding Enabled}" Value="False">
                <Setter Property="Opacity" Value="{StaticResource DisabledOpacity}" />
            </DataTrigger>
            <DataTrigger Binding="{Binding OptionType}" Value="POINT_AND_CLICK_FREQUENCY">
                <Setter Property="Style" Value="{StaticResource PointNClickSliderValueStyle}" TargetName="PART_ValueLabel"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding OptionType}" Value="DIALOG_FONT_SIZE">
                <Setter Property="Style" Value="{StaticResource FontSizeSliderValueStyle}" TargetName="PART_ValueLabel"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding OptionType}" Value="OVERHEAD_FONT_SIZE">
                <Setter Property="Style" Value="{StaticResource FontSizeSliderValueStyle}" TargetName="PART_ValueLabel"/>
            </DataTrigger>            
            <DataTrigger Binding="{Binding OptionType}" Value="BOOK_FONT_SIZE">
                <Setter Property="Style" Value="{StaticResource FontSizeSliderValueStyle}" TargetName="PART_ValueLabel"/>
                <Setter Property="Text" TargetName="PART_ValueLabel">
                    <Setter.Value>
                        <MultiBinding Converter="{StaticResource ParameterizedTranslatedStringConverter}">
                            <Binding Source="h1169e44bg06ddg4592g8016gfaa0943514c6"/>
                            <Binding Path="Tag" RelativeSource="{RelativeSource Mode=Self}" Converter="{StaticResource AddConverter}" ConverterParameter="{StaticResource IntermediateFontSize}"/>
                        </MultiBinding>
                    </Setter.Value>
                </Setter>
            </DataTrigger>
            <DataTrigger Binding="{Binding OptionType}" Value="PANNING_SPEED">
                <Setter Property="Style" Value="{StaticResource CameraPanningSpeedSliderValueStyle}" TargetName="PART_ValueLabel"/>
            </DataTrigger>
            
            <DataTrigger Binding="{Binding OptionType}" Value="UIScaling">
                <Setter Property="Style" Value="{StaticResource FontSizeSliderValueStyle}" TargetName="PART_ValueLabel"/>
                <Setter Property="Text" Value="{Binding Value, Converter={StaticResource EnumTranslatedStringConverter}, ConverterParameter='h2e12e1f7g7318g4704ga3efgf4121ab0027a'}" TargetName="PART_ValueLabel"/>
            </DataTrigger>

            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="bg" Property="ImageSource" Value="pack://application:,,,/Core;component/Assets/Options/sliderBox_wide_h.png"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="HUI_CCSliderStyle" TargetType="{x:Type Slider}">
        <Setter Property="Template" Value="{StaticResource HUI_SliderTemplate}"/>
        <Setter Property="Value" Value="{Binding Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
        <Setter Property="Minimum" Value="{Binding Min}"/>
        <Setter Property="Maximum" Value="{Binding Max}"/>
        <Setter Property="IsSnapToTickEnabled" Value="False"/>
        <Setter Property="SmallChange" Value="1"/>
        <Setter Property="LargeChange" Value="1"/>
        <Setter Property="IsEnabled" Value="{Binding Enabled}"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
    </Style>

    <!-- HUI Text Input Template -->
    <!-- 
      Usage:
        <ContentControl Template="{StaticResource HUI_NameInputTemplate}" 
            Tag="{Binding Source='h5cd3cabag657ag4abfga33fg9b1b2808ee7f', Converter={StaticResource TranslatedStringConverter}}"/>

    -->
    <ControlTemplate x:Key="HUI_NameInputTemplate">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,20">
            <Grid>
                <StackPanel>
                    <ContentControl x:Name="textSubheader" Template="{StaticResource HUI_Decorated_Subheader}" Margin="0,50,0,0">
                        <TextBlock x:Name="headerText" 
                                   Text="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" 
                                   Style="{StaticResource AppearanceCarouselTitleText}"/>
                    </ContentControl>

                    <ContentControl x:Name="textInputBox" HorizontalAlignment="Center" ls:MoveFocus.Focusable="True" Focusable="True" Template="{StaticResource CarouselButtonTemplate}">
                        <b:Interaction.Triggers>
                            <b:DataTrigger Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=textInputBox}" Value="True">
                                <ls:BringIntoViewAction TargetName="textSubheader"/>
                            </b:DataTrigger>
                        </b:Interaction.Triggers>
                        <Grid MinWidth="740" MinHeight="256">
                            <!-- UIAccept input binding to focus the text input -->
                            <ls:LSInputBinding BoundEvent="UIAccept" IsEnabled="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=textInputBox}">
                                <b:Interaction.Triggers>
                                    <b:EventTrigger EventName="LSInputBindingPressed">
                                        <ls:SetMoveFocusAction TargetName="CharacterCreation_c" Reset="True"/>
                                        <noesis:SetFocusAction TargetName="textInput"/>
                                    </b:EventTrigger>
                                </b:Interaction.Triggers>
                            </ls:LSInputBinding>
                            
                            <!-- Main input background -->
                            <ls:LSNineSliceImage x:Name="textInputBg" Padding="20,0,20,0" Slices="150,86,150,86" ImageSource="pack://application:,,,/Core;component/Assets/CC/btn_namegroupBG_01.png" MinWidth="740" MinHeight="256">
                            </ls:LSNineSliceImage>
                            
                            <!-- Animated border inside input area when focused -->
                            <Control x:Name="innerAnimatedBorder" Visibility="Collapsed"
                                     Style="{StaticResource FocusableBorderCarouselStyle}"
                                     Height="126" Width="1015"
                                     HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            
                            <ls:LSTextBox x:Name="textInput" 
                                          Text="{Binding CharacterName, Mode=TwoWay}" 
                                          IsEnabled="{Binding CanEditName}" 
                                          MaxLength="20" 
                                          TextImmediateSourceUpdate="True" 
                                          Foreground="{StaticResource CCTextPrimary}" 
                                          FontSize="{StaticResource MassiveFontSize}" 
                                          TextAlignment="Center" 
                                          HorizontalContentAlignment="Center" 
                                          VerticalAlignment="Center" 
                                          Margin="60,0,60,0"/>
                            
                            <!-- Focus Input Button inside the text input box -->
                            <ls:LSButton x:Name="focusInput" 
                                         IsEnabled="{Binding CanEditName}" 
                                         Visibility="Collapsed" 
                                         Content="{Binding CurrentPlayer.UIData.InputEvents, ConverterParameter=UIAccept, Converter={StaticResource FindInputEventConverter}}" 
                                         HorizontalAlignment="Right" VerticalAlignment="Center"
                                         Margin="0,0,80,0">
                                <ls:LSButton.ContentTemplate>
                                    <DataTemplate DataType="ls:VMInputEvent">
                                        <Control x:Name="ImageSwitcher" Template="{StaticResource ControllerButton.ImageSwitcher}" VerticalAlignment="Center" Width="{StaticResource ScaledControllerHintSizeSmall}" Height="{StaticResource ScaledControllerHintSizeSmall}"/>
                                    </DataTemplate>
                                </ls:LSButton.ContentTemplate>
                                <b:Interaction.Triggers>
                                    <b:EventTrigger EventName="Click">
                                        <ls:SetMoveFocusAction TargetName="CharacterCreation_c" Reset="True"/>
                                        <noesis:SetFocusAction TargetName="textInput"/>
                                    </b:EventTrigger>
                                </b:Interaction.Triggers>
                            </ls:LSButton>
                        </Grid>
                    </ContentControl>
                </StackPanel>
            </Grid>
        </StackPanel>
        <ControlTemplate.Triggers>
            <!-- Default MaxLength if not specified -->
            <DataTrigger Binding="{Binding InputMaxLength}" Value="{x:Null}">
                <Setter TargetName="textInput" Property="MaxLength" Value="20"/>
            </DataTrigger>
            
            <!-- Show animated border inside input when textbox has focus -->
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding IsFocused, ElementName=textInput}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="innerAnimatedBorder" Property="Visibility" Value="Visible"/>
                <Setter TargetName="focusInput" Property="Visibility" Value="Visible"/>
            </MultiDataTrigger>
            <!-- Show animated border inside input when textInputBox has controller focus -->
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=textInputBox}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="innerAnimatedBorder" Property="Visibility" Value="Visible"/>
                <Setter TargetName="focusInput" Property="Visibility" Value="Visible"/>
            </MultiDataTrigger>
            <!-- Show animated border inside input when focusInput button has focus -->
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=focusInput}" Value="True"/>
                </MultiDataTrigger.Conditions>
                <Setter TargetName="innerAnimatedBorder" Property="Visibility" Value="Visible"/>
                <Setter TargetName="focusInput" Property="Visibility" Value="Visible"/>
            </MultiDataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

</ResourceDictionary>
