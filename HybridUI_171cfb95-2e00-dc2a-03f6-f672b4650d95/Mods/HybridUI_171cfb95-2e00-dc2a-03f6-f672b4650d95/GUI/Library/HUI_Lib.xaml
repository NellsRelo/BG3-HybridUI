                                        <DataTrigger Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=base}" Value="True">
                                            <Setter TargetName="shuffleInput" Property="IsEnabled" Value="True"/>
                                        </DataTrigger>
                                        
