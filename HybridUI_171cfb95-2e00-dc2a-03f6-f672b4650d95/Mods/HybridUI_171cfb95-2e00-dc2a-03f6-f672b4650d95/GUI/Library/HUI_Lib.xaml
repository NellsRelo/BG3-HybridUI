                                            <Setter TargetName="shuffleActionContent" Property="Visibility" Value="Visible"/>
                                            <Setter TargetName="shuffleButton" Property="Opacity" Value="1"/>
                                        </MultiDataTrigger>
                                        
                                        <!-- Hide shuffle action when no items -->
                                        <DataTrigger Binding="{Binding ItemsSource.Count, RelativeSource={RelativeSource AncestorType={x:Type ListBox}}}" Value="0">
                                            <Setter TargetName="shuffleActionContent" Property="Visibility" Value="Collapsed"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding ItemsSource.Count, RelativeSource={RelativeSource AncestorType={x:Type ListBox}}}" Value="1">
                                            <Setter TargetName="shuffleActionContent" Property="Visibility" Value="Collapsed"/>
                                        </DataTrigger>

