    <!-- HUI Separate Expanded Grid Template -->
    <ControlTemplate x:Key="HUI_CarouselExpandedGrid" TargetType="ListBox">
        <Border MaxHeight="700" HorizontalAlignment="Center" VerticalAlignment="Top" Focusable="False" Margin="0,0,0,20" ClipToBounds="True">
            <StackPanel Style="{StaticResource gameplayScrolledStackPanelStyle}" VerticalAlignment="Stretch" Focusable="False" >
                        <ListBox x:Name="expandedItemGrid" 
                                ItemsSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ItemsSource}"
                                SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedItem, Mode=TwoWay}"
                                Style="{StaticResource HUI_AppearanceIconGridStyle}"
                                IsSynchronizedWithCurrentItem="True"
                                KeyboardNavigation.DirectionalNavigation="Continue"
                                IsTabStop="True">
                        <b:Interaction.Triggers>
                            <b:PropertyChangedTrigger Binding="{Binding ElementName=expandedItemGrid, Path=SelectedItem}">
                                <ls:LSPlaySound Sound="UI_DefaultButton_Click"/>
                            </b:PropertyChangedTrigger>
                        </b:Interaction.Triggers>
                    </ListBox>

                    <!-- Collapse Grid Input Binding - Set grid to closed state -->
                    <ls:LSInputBinding BoundEvent="UICancel" IsEnabled="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsVisible}">
                        <b:Interaction.Triggers>
                            <b:EventTrigger EventName="LSInputBindingPressed">
                                <b:ChangePropertyAction TargetObject="{Binding RelativeSource={RelativeSource TemplatedParent}}" PropertyName="Tag" Value="GridClosed"/>
                                <b:ChangePropertyAction TargetObject="{Binding RelativeSource={RelativeSource TemplatedParent}}" PropertyName="Visibility" Value="Collapsed"/>
                                <ls:LSPlaySound Sound="UI_DefaultButton_Click"/>
                            </b:EventTrigger>
                        </b:Interaction.Triggers>
                    </ls:LSInputBinding>
                </StackPanel>
        </Border>
    </ControlTemplate>

