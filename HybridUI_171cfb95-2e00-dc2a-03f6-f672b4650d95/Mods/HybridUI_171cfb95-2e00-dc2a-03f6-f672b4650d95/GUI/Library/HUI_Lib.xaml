                                            <Setter TargetName="rightBtn" Property="Visibility" Value="Collapsed"/>
                                        </DataTrigger>
                                        
                                        <!-- Disable carousel and buttons when grid is visible AND carousel is not focused -->
                                        <DataTrigger Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=base}" Value="False">
                                            <Setter TargetName="leftBtn" Property="IsEnabled" Value="False"/>
                                            <Setter TargetName="rightBtn" Property="IsEnabled" Value="False"/>
                                        </DataTrigger>

                                        <!-- Show expand button when focused and grid is not open -->
                                        <DataTrigger Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=base}" Value="True">
                                            <Setter TargetName="expandButton" Property="Opacity" Value="1"/>
                                        </DataTrigger>
                                        


                                        <!-- Show shuffle action when focused and has multiple items available -->
                                        <MultiDataTrigger>
                                            <MultiDataTrigger.Conditions>
                                                <Condition Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=base}" Value="True"/>
                                            </MultiDataTrigger.Conditions>
                                            <Setter TargetName="leftActionArea" Property="Opacity" Value="1"/>
                                            <Setter TargetName="shuffleActionContent" Property="Visibility" Value="Visible"/>
                                            <Setter TargetName="shuffleButton" Property="Opacity" Value="1"/>
                                        </MultiDataTrigger>
                                        
                                        <!-- Hide shuffle action when no items -->
                                        <DataTrigger Binding="{Binding ItemsSource.Count, RelativeSource={RelativeSource AncestorType={x:Type ListBox}}}" Value="0">
                                            <Setter TargetName="shuffleActionContent" Property="Visibility" Value="Collapsed"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding ItemsSource.Count, RelativeSource={RelativeSource AncestorType={x:Type ListBox}}}" Value="1">
                                            <Setter TargetName="shuffleActionContent" Property="Visibility" Value="Collapsed"/>
                                        </DataTrigger>

                                        <!-- Enable shuffle input when focused (matches original HUI_CarouselTemplate logic) -->
                                        <DataTrigger Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=base}" Value="True">
                                            <Setter TargetName="shuffleInput" Property="IsEnabled" Value="True"/>
                                        </DataTrigger>
                                        
                                        <!-- Disable shuffle input when no items or only one item -->
                                        <DataTrigger Binding="{Binding ItemsSource.Count, RelativeSource={RelativeSource AncestorType={x:Type ListBox}}}" Value="0">
                                            <Setter TargetName="shuffleInput" Property="IsEnabled" Value="False"/>
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding ItemsSource.Count, RelativeSource={RelativeSource AncestorType={x:Type ListBox}}}" Value="1">
                                            <Setter TargetName="shuffleInput" Property="IsEnabled" Value="False"/>
                                        </DataTrigger>



                                        <!-- Enable expand input for face carousels -->
                                        <MultiDataTrigger>
                                            <MultiDataTrigger.Conditions>
                                                <Condition Binding="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Value="faces"/>
                                                <Condition Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=base}" Value="True"/>
                                            </MultiDataTrigger.Conditions>
                                            <Setter TargetName="faceExpandInput" Property="IsEnabled" Value="True"/>
                                        </MultiDataTrigger>
                                        
                                        <!-- Enable expand input for hair carousels -->
                                        <MultiDataTrigger>
                                            <MultiDataTrigger.Conditions>
                                                <Condition Binding="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Value="hairs"/>
                                                <Condition Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=base}" Value="True"/>
                                            </MultiDataTrigger.Conditions>
                                            <Setter TargetName="hairExpandInput" Property="IsEnabled" Value="True"/>
                                        </MultiDataTrigger>
                                        
                                        <!-- Enable expand input for facial hair carousels -->
                                        <MultiDataTrigger>
                                            <MultiDataTrigger.Conditions>
                                                <Condition Binding="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Value="facialHairs"/>
                                                <Condition Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=base}" Value="True"/>
                                            </MultiDataTrigger.Conditions>
                                            <Setter TargetName="facialHairExpandInput" Property="IsEnabled" Value="True"/>
                                        </MultiDataTrigger>
                                        
                                        <!-- Enable expand input for makeup carousels -->
                                        <MultiDataTrigger>
                                            <MultiDataTrigger.Conditions>
                                                <Condition Binding="{Binding Tag, RelativeSource={RelativeSource TemplatedParent}}" Value="makeup"/>
                                                <Condition Binding="{Binding Path=(ls:MoveFocus.IsFocused), ElementName=base}" Value="True"/>
                                            </MultiDataTrigger.Conditions>
                                            <Setter TargetName="makeupExpandInput" Property="IsEnabled" Value="True"/>
                                        </MultiDataTrigger>
                                        


                                        <!-- Facial Hair Carousel needs CarouselButton Appearance -->
                                        <DataTrigger Binding="{Binding Name, RelativeSource={RelativeSource TemplatedParent}}" Value="facialHairCarousel">
                                            <Setter TargetName="base" Property="Template" Value="{StaticResource CarouselButtonTemplate}"/>
                                        </DataTrigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </ListBox.Template>
                        </ListBox>

