                                        </MultiDataTrigger>
                                        
                                        <!-- Show collapse button when grid is visible -->
                                        <DataTrigger Binding="{Binding ElementName=gridScrollViewer, Path=Visibility}" Value="Visible">
                                            <Setter TargetName="collapseButton" Property="Opacity" Value="1"/>
