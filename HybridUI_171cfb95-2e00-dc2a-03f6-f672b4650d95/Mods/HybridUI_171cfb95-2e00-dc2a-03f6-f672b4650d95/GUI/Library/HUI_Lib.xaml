                            <b:Interaction.Triggers>
                                <b:DataTrigger Binding="{Binding Path=(ls:MoveFocus.IsFocused), RelativeSource={RelativeSource Self}}" Value="True">
                                    <ls:BringIntoViewAction TargetName="carouselDisplay"/>
                                </b:DataTrigger>
                            </b:Interaction.Triggers>
